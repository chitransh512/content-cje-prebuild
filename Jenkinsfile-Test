pipeline {
    agent any
    stages {
    
    stage('Deploy-from-featurebranch') {
            when {
		        expression {
		        return env.GIT_BRANCH == "origin/master" 

                }   
            }
            steps {
                echo "Branch=Master"
		    }   
        }
	
	
	stage('Deploy-from-featurebranch') {
            when {
		        expression {
		        return env.GIT_BRANCH == "origin/develop"

                }   
            }
            steps {
                echo "Branch=Deveelop"
                 
		    }   
        }
}
}

