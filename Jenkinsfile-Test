pipeline {
    agent any
    
    stages {
        stage('For mastere') {
            steps {
                when {
                branch 'production'
            }
                sh """
                    ls -la
                """
            }
        }
    

        stage('for develop') {         
            
            steps {
                when {
                branch 'development'
            }
                    sh """ 
                    ls -la
                    """
                }
        }
    }
}
